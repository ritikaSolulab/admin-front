
<style>
    .slider.round {
        border-radius: 34px;
        width: 42px;
        min-width: inherit;
        margin-left: -6px;
    }

    .slider {
        position: absolute;
        cursor: pointer;
        inset: 0px;
        background-color: rgb(204, 204, 204);
        transition: all 0.4s ease 0s;
    }
</style>
<div class="row">
    <div class="tab-pane fade show active" id="pro" role="tabpanel" aria-labelledby="del-tab">
        <div class="row m-b-sm ">
            <div class="col-md-8 ">
                <div class="bootstrap-tagsinput" id="myDropdown">
                    <div class="btn-group dropdown-filter">
                        <button type="button" class="btn dropdown-toggle m-r-sm" data-bs-toggle="dropdown"
                            aria-haspopup="true" aria-expanded="false">
                            <i class="fa fa-filter" aria-hidden="true"></i> Filter by
                            <span class="caret"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li class="dropdown-actions d-flex-between">
                                <div>
                                    <button onclick="filterApi()" type="button"
                                        class="btn btn-success btn-xs">Apply</button>
                                    <button onclick="resetFilterApi()" type="button"
                                        class="btn btn-default btn-xs">Reset</button>
                                </div>
                                <a onclick="closeFilter()">
                                    <i class="fa fa-times" aria-hidden="true" style="color: #000;font-size: large;
                                        margin-top: 4px;"></i></a>
                            </li>
                            <li role="separator" class="divider"></li>

                            <li>
                                <div class="row">
                                    <div class="col-lg-6 col-sm-6 col-md-6">
                                        <label>Age</label>
                                        <select id="last-30-days" class="form-control ">
                                            <option value="">
                                                Select
                                            </option>

                                            <option value="Last 30 days">
                                                Last 30 days
                                            </option>
                                        </select>
                                    </div>
                                    <div class="col-lg-6   col-md-6 col-sm-6">
                                        <label>Profile</label>
                                        <select id="select-profile" class="form-control ">
                                            <option value="">
                                                Select
                                            </option>
                                            <option value="Creator">Creator</option>
                                            <option value="tokenz-x">Toekenz X</option>
                                            <option value="tokenz-kids">Toekenz Kidz
                                            </option>
                                        </select>
                                    </div>
                                </div>

                            </li>
                            <li>
                                <div class="row">
                                    <div class="col-lg-6 col-sm-6  col-md-6">
                                        <label>Method</label>
                                        <select id="select-method" class="form-control ">
                                            <option value="">
                                                Select
                                            </option>
                                            <option value="Buy">Buy</option>
                                            <option value="PutOnSale">Sale</option>
                                            <option value="Gift">Gift</option>
                                            <option value="Donate">Donate</option>
                                        </select>
                                    </div>
                                    <div class="col-lg-6  col-md-6 col-sm-6">
                                        <label>Status</label>
                                        <select id="select-status" class="form-control ">
                                            <option value="">
                                                Select
                                            </option>
                                            <option value="Success">Success</option>
                                            <option value="Pending">Pending</option>
                                            <option value="Failed">Failed</option>
                                        </select>
                                    </div>
                                </div>



                            </li>
                            <li>

                                <div class="form-group">
                                    <label for="formControlRange">Value</label>

                                    <div class="d-flex-around">
                                        <div>

                                            <input id="startAmountRange" type="text" placeholder="0" disabled value=""
                                                class="form-control">
                                        </div>
                                        <input type="range" id="amount_slider" class="form-control-range m-r-sm m-l-sm"
                                            value="0" min="0" max="50000" step="100"
                                            onchange="updateTextInput(this.value);">
                                        <div>

                                            <input id="endAmountRange" type="text" class="form-control " value=""
                                                placeholder="Max" disabled>
                                        </div>
                                    </div>
                                </div>
                            </li>
                            <li>

                                <div class="form-group">
                                    <label for="formControlRange">Txn Fees</label>

                                    <div class="d-flex-around">
                                        <div>

                                            <input id="startTxnRange" type="text" value="" class="form-control"
                                                placeholder="0" disabled>
                                        </div>
                                        <input type="range" class="form-control-range m-r-sm m-l-sm" id="txn_slider"
                                            value="0" min="0" max="50000" step="100"
                                            onchange="updateTextInputTxn(this.value);">
                                        <div>

                                            <input type="text" class="form-control " id="endTxnRange" placeholder="Max"
                                                value="" disabled>
                                        </div>
                                    </div>
                                </div>
                            </li>

                            <li role="separator" class="divider"></li>
                            <li class="" id="data_5">
                                <div class="row">
                                    <div class="col-lg-6">
                                        <label>From</label>
                                        <div class="input-daterange input-group" id="datepicker">
                                            <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                            <input id="startDate" type="text" class="input-sm form-control" name="start"
                                                placeholder="Please select a date" autocomplete="off">
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <label>To</label>
                                        <div class="input-daterange input-group" id="datepicker">
                                            <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                            <input id="endDate" type="text" class="input-sm form-control" name="start"
                                                placeholder="Please select a date" autocomplete="off">
                                        </div>
                                    </div>
                            </li>

                        </ul>
                    </div>

                    <span id="filter-profile" class="tag label label-primary hide">

                        <a onclick="resetProfileFilter()" style="color: #fff;"><i class="fa fa-times"
                                aria-hidden="true"></i></a>
                    </span>
                    <span id="filter-method" class="tag label label-primary hide">

                        <a onclick="resetMethodFilter()" style="color: #fff;"><i class="fa fa-times"
                                aria-hidden="true"></i></a>
                    </span>
                    <span id="filter-status" class="tag label label-primary hide">

                        <a onclick="resetStatusFilter()" style="color: #fff;"><i class="fa fa-times"
                                aria-hidden="true"></i></a>
                    </span>
                    <span id="filter-lastMonth" class="tag label label-primary hide">

                        <a onclick="resetDateFilter()" style="color: #fff;"><i class="fa fa-times"
                                aria-hidden="true"></i></a>
                    </span>
                    <span id="filter-amount" class="tag label label-primary hide">

                        <a onclick="resetAmount()" style="color: #fff;"><i class="fa fa-times"
                                aria-hidden="true"></i></a>
                    </span>
                    <span id="filter-txn" class="tag label label-primary hide">

                        <a onclick="resetTxn()" style="color: #fff;"><i class="fa fa-times" aria-hidden="true"></i></a>
                    </span>
                </div>
            </div>
        </div>
        <div class="table-responsive">

            <table class="table table-striped table-bordered table-hover " id="transactionTable">
                <thead>
                    <tr>
                        <th>Sr No</th>
                        <th>Txn Hash</th>
                        <th>Status</th>
                        <th>Seller ID</th>
                        <th>Buyer ID</th>
                        <th>Section</th>
                        <th>Item</th>
                        <th>Type</th>
                        <th>Value</th>
                        <th>Payment Mode</th>
                        <th>Date</th>
                        <th>Fees</th>
                    </tr>
                </thead>
                <tbody>

                </tbody>
            </table>
        </div>

    </div>
</div>

<script type="text/javascript">
    $(document).on('click', '.dropdown-menu li', function (e) {
        console.log("click")
        e.stopPropagation();
    }); $(document).on('click', '.dropdown-menu li  option', function (e) {
        e.stopPropagation();
    });
    $('#myDropdown .dropdown-menu').on({
        "click": function (e) {
            e.stopPropagation();
        }
    });
</script>