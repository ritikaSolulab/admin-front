<%- include('./style')%>
    <div class="card-body">
        <div class="row">
            <form>
                <div class="row mb-3">
                    <label for="inputEmail3" class="col-sm-3 col-form-label">Enter Details</label>
                    <div class="col-sm-3">
                        <input type="text" class="form-control" id="inputEmail3" placeholder="Name">
                    </div>
                </div>
                <div class="row mb-3">
                    <label for="inputEmail3" class="col-sm-3 col-form-label"></label>
                    <div class="col-sm-3">
                        <input type="text" class="form-control" id="inputPassword3" placeholder="Profession">
                    </div>
                </div>
                <div class="row mb-3">
                    <label for="inputEmail3" class="col-sm-3 col-form-label">Banner Image or Video</label>
                    <div class="col-sm-3" style="margin-top: 10px;">
                        <input type="file" class="form-control" id="inputEmail3" placeholder="Name">
                    </div>
                </div>
                <div class="row mb-3">
                    <label for="inputEmail3" class="col-sm-13 col-form-label" style="margin-top: 15px;">Select Banner
                        Text Color</label>
                        <div class="col-lg-3 col-3 offset-md-3" id="color-picker">
                            <div class="color-palette">
                                <div class="color" style="background-color: #FF0000;"></div>
                                <div class="color" style="background-color: #00FF00;"></div>
                                <div class="color" style="background-color: #0000FF;"></div>
                                <div class="color" style="background-color: #FFFF00;"></div>
                                <div class="color" style="background-color: #FF00FF;"></div>
                                <div class="color" style="background-color: #00FFFF;"></div>
                            </div>
                        </div>

                </div>
        <!-- </div> -->
        <div class="row mb-3" style="margin-top: 20px;">
            <label for="inputEmail3" class="col-sm-3 col-form-label">Content</label>
            <div class="col-sm-3" style="margin-top: 10px;">
                <div class="col-sm-3" id="editor"></div>
            </div>

        </div>

        <div class="row mb-3">
            <div class="tab-content" id="nav-tabContent">
                <div style="margin-top: 60px;">
                    <label for="inputEmail3" class="col-sm-3 col-form-label">NFT List</label>
                </div>
                <div class="tab-pane fade show active col-md-8 offset-md-3"
                    id="nft-table" role="tabpanel" aria-labelledby="nft-table-tab">
                    <div class="row">
                        <table class="table table-striped table-bordered table-hover visible" id="nftTable">
                            <thead>
                                <tr>
                                <tr>
                                    <th>Sr No</th>
                                    <th>NFT Name</th>
                                    <th>Creator</th>
                                    <th>Owner</th>
                                    <th>Collection Name</th>
                                    <th>Price($)</th>
                                    <th>Action</th>
                                </tr>
                                </tr>
                            </thead>

                        </table>
                    </div>


                </div>


            </div>
        </div>

        <div class="row mb-3">
            <div style="display: flex;">
                <label for="inputEmail3" class="col-sm-3 col-form-label" style="margin-top: 30px;">Profile</label>
                <div style="margin-top: 40px; width: 30%;">
                    <select class="form-select" id="multiple-select-field" data-placeholder="Choose anything" multiple>
                        <option>Christmas Island</option>
                        <option>South Sudan</option>
                        <option>Jamaica</option>
                        <option>Kenya</option>
                        <option>French Guiana</option>
                        <option>Mayotta</option>
                        <option>Liechtenstein</option>
                        <option>Denmark</option>
                        <option>Eritrea</option>
                        <option>Gibraltar</option>

                    </select>
                </div>


            </div>
        </div>

        <div class="col-12" style="margin-top: 40px;">
            <button type="submit" class="btn btn-primary _button">Update</button>
        </div>

    </div>

    </form>
    </div>

    </div>

