                           <input style="height: 100px;" type="file" class="form-control" id="inputEmail3" placeholder="Name">
                         </div>
                         <div class="col-sm-3">
                             <label for="inputEmail3" class="col-sm-9 col-form-label">Select Banner Text Color</label>
                             <input type="color" style="height: 40px;" class="form-control" id="inputEmail3" placeholder="Name">
