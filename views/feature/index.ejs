                     </div>
                     <div class="row mb-3">
                         <label for="inputEmail3" class="col-sm-3 col-form-label"></label>
                         <div class="col-sm-3" id="editor"></div>                
                     </div>
                     <div class="row mb-3">
                        <label for="inputEmail3" class="col-sm-3 col-form-label">Profile Links</label>
                        <div class="col-sm-6">
                          <input type="text" class="form-control" id="inputEmail3" placeholder="Add profile links">
                        </div>
                    </div>
                    <div class="col-12">
                        <button type="submit" class="btn btn-primary _button">Update</button>
                    </div>
                        
                    </div>
                     
                   </form>
             </div>
         
         </div>
         
         <%- include('../partials/datatable-script')%>
