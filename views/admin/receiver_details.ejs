<!DOCTYPE html>
<html>

<head>

    <%- include('../partials/style.ejs') %>
</head>

<body>
    <div id="wrapper">
        <%- include('../partials/menu.ejs') %>

        <!-- side bar end -->
        <div id="page-wrapper" class="gray-bg">
            <!-- top nav start -->
            <%- include('../partials/header.ejs') %>

            <!-- top nav end -->
            <div class="wrapper wrapper-content profile-page">
                <div class="ibox ">
                    <div class="ibox-title">
                        <a href="/admin/receiver_management" class="back-arw"><i class="fa fa-arrow-circle-left"
                                aria-hidden="true"></i></a>
                        <h5><%= receiverName %> Details</h5>
                        <div class="ibox-tools">
                            <button class="btn btn-primary" type="button" data-toggle="modal"
                                 id="deActivateAccount" style="display: none;" onclick="deactivateUser()">deactivate account</button>
                                <button class="btn btn-primary" type="button" data-toggle="modal"
                                 id="activateAccount"  style="display: none;" onclick="activateUser()">activate account</button>
                        </div>
                    </div>
                    <div class="ibox-content ">
                        <div class="row  align-items-center">
                            <div class="col-md-12">
                                <div class="profile-image">
                                    <img src="" class="rounded-circle circle-border" alt="profile" id="profile">
                                </div>
                                <div class="profile-info">
                                    <div class="">
                                        <div>
                                            <h2 class="no-margins name" >
                                                
                                            </h2>
                                            <h4 class="emailShow"></h4>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- row end -->
                        <div class="row mt-3">
                            <div class="col-md-12">
                                <div class="table-tab">
                                    <ul class="nav nav-pills mb-4 " id="pills-tab" role="tablist">
                                        <li class="nav-item">
                                            <a class="nav-link active" id="pro-tab" data-toggle="pill" href="#pro"
                                                role="tab" aria-controls="pills-home" aria-selected="true">
                                                <%= receiverName %> details
                                            </a>
                                        </li>
                                        <!-- <li class="nav-item">
                                            <a class="nav-link" id="trans-tab" data-toggle="pill" href="#trans"
                                                role="tab" aria-controls="pills-home" aria-selected="true">
                                                transactions
                                            </a>
                                        </li> -->
                                    </ul>
                                    <div class="tab-content" id="pills-tabContent">
                                        <div class="tab-pane fade show active" id="pro" role="tabpanel"
                                            aria-labelledby="del-tab">
                                            <div class="row ">
                                                <div class="col-xl-6">
                                                    <div class="card-wrapper">
                                                        <form>
                                                            <div class="form-group row">
                                                                <label
                                                                    class="col-sm-5 col-xl-5 col-form-label">Name</label>
                                                                <div class="col-sm-7  col-xl-7">
                                                                    <p class="name"></p>
                                                                </div>
                                                            </div>
                                                            <div class="form-group row">
                                                                <label
                                                                    class="col-sm-5 col-xl-5 col-form-label">Address</label>
                                                                <div class="col-sm-7     col-xl-7">
                                                                    <p id="address">Ahmedabad,Gujarat,India.</p>
                                                                </div>
                                                            </div>
                                                            <div class="form-group row">
                                                                <label class="col-sm-5  col-xl-5 col-form-label">Phone
                                                                    Number</label>
                                                                <div class="col-sm-7  col-xl-7">
                                                                    <p id="phoneNumber"></p>
                                                                </div>
                                                            </div>
                                                            <div class="form-group row">
                                                                <label class="col-sm-5  col-xl-5 col-form-label">Email
                                                                    Address</label>
                                                                <div class="col-sm-7  col-xl-7">
                                                                    <p class="emailShow"></p>
                                                                </div>
                                                            </div>
                                                            <div class="form-group row">
                                                                <label class="col-sm-5  col-xl-5 col-form-label">Login
                                                                    Via</label>
                                                                <div class="col-sm-7  col-xl-7">
                                                                    <p>Email Address</p>
                                                                </div>
                                                            </div>
                                                            <div class="form-group row">
                                                                <label class="col-sm-5  col-xl-5 col-form-label">Date of
                                                                    Registration</label>
                                                                <div class="col-sm-7  col-xl-7">
                                                                    <p id="registeredAt"></p>
                                                                </div>
                                                            </div>
                                                            <div class="form-group row">
                                                                <label
                                                                    class="col-sm-5  col-xl-5 col-form-label">Status</label>
                                                                <div class="col-sm-7  col-xl-7" id="statusShow">
                                                                </div>
                                                            </div>
                                                        </form>
                                                    </div>
                                                    <!-- card-wrapper end -->
                                                </div>
                                                <!-- col-6 end -->
                                                <div class="col-xl-6">
                                                    <div class="card-wrapper">
                                                        <form>
                                                            <div class="form-group row">
                                                                <label class="col-sm-5  col-xl-8 col-form-label">Total
                                                                    Accepted <%= documentName %>s</label>
                                                                <div class="col-sm-7  col-xl-4">
                                                                    <p id="acceptedDocTotal">0</p>
                                                                </div>
                                                            </div>
                                                            <div class="form-group row">
                                                                <label class="col-sm-5  col-xl-8 col-form-label">Total
                                                                    Rejected <%= documentName %>s</label>
                                                                <div class="col-sm-7  col-xl-4">
                                                                    <p id="rejectedDocTotal">0</p>
                                                                </div>
                                                            </div>
                                                            <div class="form-group row">
                                                                <label class="col-sm-5  col-xl-8 col-form-label">Total
                                                                    Pending <%= documentName %>s</label>
                                                                <div class="col-sm-7  col-xl-4">
                                                                    <p id="pendingDocTotal">0</p>
                                                                </div>
                                                            </div>
                                                            <div class="form-group row">
                                                                <label class="col-sm-5  col-xl-8 col-form-label">Total
                                                                    Shared <%= documentName %></label>
                                                                <div class="col-sm-7  col-xl-4">
                                                                    <p id="sharedDocTotal">0</p>
                                                                </div>
                                                            </div>
                                                            <div class="form-group row">
                                                                <label class="col-sm-5  col-xl-8 col-form-label">Total
                                                                    Updated Required <%= documentName %>s</label>
                                                                <div class="col-sm-7  col-xl-4">
                                                                    <p id="updateRequiredDocTotal">0</p>
                                                                </div>
                                                            </div>
                                                            <div class="form-group row">
                                                                <label class="col-sm-5  col-xl-8 col-form-label">Total
                                                                    Blocked <%= providerName %>s</label>
                                                                <div class="col-sm-7  col-xl-4">
                                                                    <p id="blockedProviderTotal">0</p>
                                                                </div>
                                                            </div>
                                                            <div class="form-group row">
                                                                <label class="col-sm-5  col-xl-8 col-form-label">Total
                                                                    Favourite <%= documentName %></label>
                                                                <div class="col-sm-7  col-xl-4">
                                                                    <p id="favouriteDocTotal">0</p>
                                                                </div>
                                                            </div>
                                                        </form>
                                                    </div>
                                                    <!-- card-wrapper end -->
                                                </div>
                                                <!-- col-6 end -->
                                            </div>
                                        </div>
                                        <div class="tab-pane " id="trans" role="tabpanel"
                                            aria-labelledby="pills-home-tab">
                                            <div class="row">
                                                <div class="col-md-12  mb-3 mt-3">
                                                    <!-- date-period start -->
                                                    <div class="pull-right daterange-wrapper">
                                                        <div class="date-cst d-inline-block	">
                                                            <input type="text" placeholder="DD/MM/YYYY"
                                                                class="form-control " name="daterange" readonly='true'>
                                                        </div>
                                                    </div>
                                                    <!-- date-period end -->
                                                </div>
                                            </div>
                                            <div class="table-responsive">
                                                <table
                                                    class="table table-striped table-bordered table-hover dataTables-example1"
                                                    style="width:100%">
                                                    <thead>
                                                        <tr>
                                                            <th>transaction id</th>
                                                            <th>timestamp</th>
                                                            <th><%= providerName %> name</th>
                                                            <th><%= receiverName %> name</th>

                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr>
                                                            <td> <a href="#" data-toggle="modal"
                                                                    data-target="#transactionDetails"> 1123 </a> </td>
                                                            <td>23/2/2020</td>
                                                            <td>XYZ</td>
                                                            <td>Standard</td>
                                                        </tr>
                                                        <tr>
                                                            <td> <a href="#" data-toggle="modal"
                                                                    data-target="#transactionDetails"> 1123 </a> </td>
                                                            <td>23/2/2020</td>
                                                            <td>XYZ</td>
                                                            <td>Gold</td>
                                                        </tr>
                                                        <tr>
                                                            <td> <a href="#" data-toggle="modal"
                                                                    data-target="#transactionDetails"> 1123 </a> </td>
                                                            <td>23/2/2020</td>
                                                            <td>XYZ</td>
                                                            <td>Premimum</td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>



                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <!-- wrapper end -->
                <%- include('../partials/footer.ejs') %>
                <!-- footer end -->
            </div>
            <!-- #page-wrapper end -->
        </div>



        <div class="modal fade" id="transactionDetails" tabindex="-1" role="dialog" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered transactionDetails" role="document">
                <div class="modal-content">
                    <div class="modal-body">
                        <h3 class="title">Transaction Details</h3>
                        <form id="">
                            <div class="form-group row justify-content-center otp-wrap">
                                <div class="col-sm-6">
                                    <label class="from-control">Transaction Id</label>
                                </div>
                                <div class="col-sm-6">
                                    <label> 1 </label>
                                </div>
                            </div>
                            <div class="form-group row justify-content-center otp-wrap">
                                <div class="col-sm-6">
                                    <label class="from-control">Validation Code</label>
                                </div>
                                <div class="col-sm-6">
                                    <label> 1 </label>
                                </div>
                            </div>
                            <div class="form-group row justify-content-center otp-wrap">
                                <div class="col-sm-6">
                                    <label class="from-control">Payload Proposed Hash</label>
                                </div>
                                <div class="col-sm-6">
                                    <label> 1 </label>
                                </div>
                            </div>
                            <div class="form-group row justify-content-center otp-wrap">
                                <div class="col-sm-6">
                                    <label class="from-control">Create MSP</label>
                                </div>
                                <div class="col-sm-6">
                                    <label> 1 </label>
                                </div>
                            </div>
                            <div class="form-group row justify-content-center otp-wrap">
                                <div class="col-sm-6">
                                    <label class="from-control">Endoser</label>
                                </div>
                                <div class="col-sm-6">
                                    <label> 1 </label>
                                </div>
                            </div>
                            <div class="form-group row justify-content-center otp-wrap">
                                <div class="col-sm-6">
                                    <label class="from-control">Chaincode Name</label>
                                </div>
                                <div class="col-sm-6">
                                    <label> 1 </label>
                                </div>
                            </div>
                            <div class="form-group row justify-content-center otp-wrap">
                                <div class="col-sm-6">
                                    <label class="from-control">Type</label>
                                </div>
                                <div class="col-sm-6">
                                    <label> 1 </label>
                                </div>
                            </div>
                            <div class="form-group row justify-content-center otp-wrap">
                                <div class="col-sm-6">
                                    <label class="from-control">Time</label>
                                </div>
                                <div class="col-sm-6">
                                    <label> 1 </label>
                                </div>
                            </div>
                            <div class="form-group row justify-content-center otp-wrap">
                                <div class="col-sm-6">
                                    <label class="from-control">Read</label>
                                </div>
                                <div class="col-sm-6">
                                    <label> 1 </label>
                                </div>
                            </div>
                            <div class="form-group row justify-content-center otp-wrap">
                                <div class="col-sm-6">
                                    <label class="from-control">Write</label>
                                </div>
                                <div class="col-sm-6">
                                    <label> 1 </label>
                                </div>
                            </div>

                            <div class="row justify-content-center mt-4">
                                <div class="col-sm-3 col-6">
                                    <input class="btn btn-white btn-block" type="button" onclick="resetForm('')"
                                        aria-label="Close" value="Close" data-dismiss="modal">
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <!-- #main wrapper end -->
        <!-- Mainly scripts -->
        <%- include('../partials/script.ejs') %>
        <script src="/js/custom/receiver_details.js"></script>
</body>

</html>