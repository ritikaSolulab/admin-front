<!DOCTYPE html>
<html>

<head>

    <%- include('../partials/style.ejs') %>
</head>

<body>
    <div id="wrapper">
        <%- include('../partials/menu.ejs') %>
     
        <!-- side bar end -->
        <div id="page-wrapper" class="gray-bg">
           <!-- top nav start -->
           <%- include('../partials/header.ejs') %>
     
        <!-- top nav end -->
        <div class="wrapper wrapper-content">
            <!-- table row start -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="ibox ">
                        <div class="ibox-title">
                            <a href="/admin/payment_history" class="back-arw"><i class="fa fa-arrow-circle-left" aria-hidden="true"></i></a>
                            <h5>Invoice Details</h5>
                            <div class="ibox-tools btn-right">
                                <a href="#" target="_blank" class="btn btn-primary" onclick="window.print()" ><i class="fa fa-print"></i>&nbsp;Print</a>
                            </div>
                        </div>
                        <div class="ibox-content p-xl ">
                            <div class="row ">
                                <div class="col-md-8 col-lg-6 col-sm-7">
                                    <form>
                                        <div class="form-group row">
                                            <label class="col-sm-5 col-xl-5 col-form-label">Provider Name</label>
                                            <div class="col-sm-7  col-xl-7">
                                                <p id="providerName"></p>
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-sm-5  col-xl-5 col-form-label">Email Address</label>
                                            <div class="col-sm-7  col-xl-7">
                                                <p id="providerEmail"></p>
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-sm-5  col-xl-5 col-form-label">Subscription Date</label>
                                            <div class="col-sm-7  col-xl-7">
                                                <p id="providerSubscription"></p>
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-sm-5  col-xl-5 col-form-label">Validity</label>
                                            <div class="col-sm-7  col-xl-7">
                                                <p id="validity"></p>
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-sm-5  col-xl-5 col-form-label">Expiration Date</label>
                                            <div class="col-sm-7  col-xl-7">
                                                <p id="expirySubscription"></p>
                                            </div>
                                        </div>
                                        <!-- <div class="form-group row">
                                            <label class="col-sm-5  col-xl-5 col-form-label">Auto Subscribe</label>
                                            <div class="col-sm-7  col-xl-7">
                                                <p>Yes</p>
                                            </div>
                                        </div> -->
                                        <div class="form-group row">
                                            <label class="col-sm-5  col-xl-5 col-form-label">Payment Mode</label>
                                            <div class="col-sm-7  col-xl-7">
                                                <p>card</p>
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-sm-5  col-xl-5 col-form-label">Payment Id</label>
                                            <div class="col-sm-7  col-xl-7">
                                                <p id="paymentId"></p>
                                            </div>
                                        </div>
                                    </form>
                                </div>

                            </div>

                            <div class="table-responsive m-t">
                                <table class="table invoice-table">
                                    <thead>
                                        <tr>
                                            <th>Product Id</th>
                                            <th>Product Name</th>
                                            <th>Amount</th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td id="productId"></td>
                                            <td>
                                                <div><strong id="planName"></strong></div>
                                                </td><td id="planAmount"></td>
                                        </tr>
                                        <!-- <tr>
                                            <td>123</td>
                                            <td>
                                                <div><strong>Standard plan</strong></div>
                                                </td><td>$26.00</td>
                                        </tr> -->

                                    </tbody>
                                </table>
                            </div>
                            <!-- /table-responsive -->

                            <table class="table invoice-total">
                                <tbody>
                                    <tr>
                                        <td><strong>Sub Total :</strong></td>
                                        <td id="planAmountSubTotal"></td>
                                    </tr>

                                    <tr>
                                        <td><strong>TOTAL :</strong></td>
                                        <td id="planAmountTotal"></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <!-- table row end -->
        </div>

            <!-- wrapper end -->
            <%- include('../partials/footer.ejs') %>
            <!-- footer end -->
        </div>
    </div>
  
    <%- include('../partials/script.ejs') %> 

    <script src="/js/custom/payment_details.js"></script>
    <script>
      
		</script>
</body>
</html>
