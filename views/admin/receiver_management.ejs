<!DOCTYPE html>
<html>

<head>

    <%- include('../partials/style.ejs') %>
    <style>
        tr{
            cursor: pointer;
        }
    </style>
</head>

<body>
    <div id="wrapper">
        <%- include('../partials/menu.ejs') %>
        <!-- side bar end -->
        <div id="page-wrapper" class="gray-bg">
            <!-- top nav start -->
            <%- include('../partials/header.ejs') %>
            <!-- top nav end -->
            <div class="wrapper wrapper-content profile-page">
                <div class="ibox">
                    <div class="ibox-title">
                        <h5><%= receiverName %> Management</h5>
                        <div class="ibox-tools">
							<!-- date-period start -->
							<div class="daterange-wrapper">						
								<div class="date-cst d-inline-block	">
									<input type="text" placeholder="DD/MM/YYYY" class="form-control " name="daterange" id="dateFilter" onchange="dateFilter(this.value)" readonly='true' />
								</div>								
							</div>                    
							<!-- date-period end --> 
						</div>
                    </div>
                    <div class="ibox-content "  >
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered table-hover " id="receiverTable"
                                style="width:100%" >
                                <thead>
                                    <tr>
                                        <th>id</th>
                                        <th><%= providerName %> Name</th>
                                        <th>Date of registration</th>
                                 
                                    </tr>
                                </thead>
                                <tbody id="receiver_management_table_body">
                               </tbody>
                            </table>
                        </div>

                        <!-- tab end -->
                    </div>
                    <!-- ibox-content end -->
                </div>
                <!-- ibox end-->
            </div>
            <!-- wrapper end -->
            <%- include('../partials/footer.ejs') %>
            <!-- footer end -->
        </div>
        <!-- #page-wrapper end -->
    </div>
    <!-- #main wrapper end -->
    <!-- Mainly scripts -->
    <%- include('../partials/script.ejs') %>
    <script src="/js/custom/receiver_management.js"></script>
</body>

</html>