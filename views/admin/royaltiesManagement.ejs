        <link href="https://cdn.datatables.net/1.13.4/css/dataTables.bootstrap5.min.css">
        <script>
            function toggleForm(formType) {
                const nftMarket = document.getElementById('pro');
                const nftLicensing = document.getElementById('licensing');

                if (formType === 'nftMarket') {
                    nftMarket.classList.remove('invisible')
                    nftMarket.classList.add('visible');
                    nftLicensing.classList.add('invisible');
                } else {
                    nftMarket.classList.add('invisible');
                    nftLicensing.classList.add('visible');
                }
            }
        </script>
        <style>
            .visible {
                display: flexbox;
                width: 100%;
            }

            .invisible {
                display: none;
            }

            .content {
                border: 2px solid;
                padding: 6px;
                display: inline-block;
                margin-left: 0;
            }
        </style>
</head>

