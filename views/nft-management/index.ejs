<%#-include('. /style')%>
    <div class="card-body">
        <div class="row">

            <nav>
                <div class="nav nav-tabs" id="nav-tab" role="tablist">
                    <button class="nav-link active" id="nav-user-tab" data-bs-toggle="tab" data-bs-target="#nav-user"
                        type="button" role="tab" aria-controls="nav-home" aria-selected="true">Collection</button>
                    <button class="nav-link" id="nav-role-tab" data-bs-toggle="tab" data-bs-target="#nav-role"
                        type="button" role="tab" aria-controls="nav-profile" aria-selected="false">NFTs</button>
                </div>
            </nav>
            <hr>
            <div class="tab-content" id="nav-tabContent">
                <div class="tab-pane fade show active" id="nav-user" role="tabpanel" aria-labelledby="nav-user-tab">
                    <div class="row">
                        <table class="table table-responsive table-striped table-bordered table-hover visible"
                            id="collectionTable" style="width: 100%;">
                            <thead>
                                <tr>
                                    <th>Sr No</th>
                                    <th>Collection</th>
                                    <th>Creator</th>
                                    <th>Owner</th>
                                    <th>Date of Listing</th>
                                    <th>Floor Price($)</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>

                <div class="tab-pane fade" id="nav-role" role="tabpanel" aria-labelledby="nav-role-tab">
                    <div class="row">
                        <table class="table table-striped table-bordered table-hover visible" id="nftTable"
                            style="width: 100%;">
                            <thead>
                                <tr>
                                    <th>Sr No</th>
                                    <th>NFT name</th>
                                    <th>Creator</th>
                                    <th>Owner</th>
                                    <th>Collection Name</th>
                                    <th>Date of Listing</th>
                                    <th>Price($)</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>

        </div>
    </div>

    <!-- Modals -->
    <div class="modal fade" data-bs-backdrop="static" data-bs-keyboard="false" id="Archive_Confirm_Modal" tabindex="-1"
        role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div style="background-color: white;" class="modal-content">
                <div class="modal-body text-center">

                    <h3 id="ArchiveConfirmText" class="title"> Are you sure you want to make this collection as archive?</h3>
                    <img src="images/backgrounds/rocket.png" alt="success">
                    <p class="head-text" id="successMessageBox"></p>
                    <button id="UpdateArchive" type="button" class="btn btn-block btn-primary">Confirm</button>
                    <button type="button" class="btn btn-block btn-primary" data-bs-dismiss="modal">Cancel</button>
                </div>
            </div>
        </div>
</div>
    <!--Modal Ends -->
