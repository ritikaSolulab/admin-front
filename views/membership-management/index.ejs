<div class="card-body">
    <div class="row">

        <nav>
            <div class="nav nav-tabs" id="nav-tab" role="tablist">
                <button class="nav-link active" id="nav-user-tab" data-bs-toggle="tab"
                    data-bs-target="#nav-user" type="button" role="tab" aria-controls="nav-home"
                    aria-selected="true">License Management</button>
                <button class="nav-link" id="nav-role-tab" data-bs-toggle="tab"
                    data-bs-target="#nav-role" type="button" role="tab"
                    aria-controls="nav-profile" aria-selected="false">Membership Management</button>
            </div>
        </nav>
        <hr>
        
        <div class="tab-content" id="nav-tabContent">
            <div class="tab-pane fade show active" id="nav-user" role="tabpanel"
                aria-labelledby="nav-user-tab">
                <div style="display: flex;">
                    <div class="tab-pane fade show active" id="pro" role="tabpanel" aria-labelledby="del-tab">
                        
                    </div>
                    
                  
                </div>
                <div class="row p-4">
                    <table class="table table-striped table-bordered table-hover visible"
                        id="LicenseTable">
                        <thead>
                            <tr>
                                <th>Sr No</th>
                                                <th>NFT Name</th>
                                                <th>Creator ID</th>
                                                <th>Date of Listing</th>
                                                <th>Total Users</th>
                                                <th>Active Users</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
            </div>

            <div class="tab-pane fade" id="nav-role" role="tabpanel"
                aria-labelledby="nav-role-tab">
               
                <div class="row p-4">
                    <table class="table table-striped table-bordered table-hover visible"
                        id="MembershipTable" >
                        <thead>
                            <tr>
                                <th>Sr No</th>
                                                <th>NFT Name</th>
                                                <th>Creator ID</th>
                                                <th>Date of Listing</th>
                                                <th>Total Users</th>
                                                <th>Active Users</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

    </div>
</div>
<%- include('../partials/datatable-script') %>
<script>
    $(document).ready(() => {
        $('#LicenseTable').DataTable({
            ordering: false
        })
        $('#MembershipTable').DataTable({
            ordering: false
        })
    })
</script>