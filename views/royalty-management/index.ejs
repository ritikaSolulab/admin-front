                                <th>NFT Name</th>
                                <th>Creator ID</th>
                                <th>Owner ID</th>
                                <th>Date of Listing</th>
                                <th>Total Royalities</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
            </div>

            <div class="tab-pane fade" id="nav-role" role="tabpanel"
